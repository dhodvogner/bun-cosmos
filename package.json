    "bundler"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhodvogner/bun-cosmos.git"
  },
  "bugs": {
    "url": "https://github.com/dhodvogner/bun-cosmos/issues"
  },
  "homepage": "https://github.com/dhodvogner/bun-cosmos#readme",
